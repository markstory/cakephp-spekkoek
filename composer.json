{
    "name": "markstory/cakephp-spekkoek",
    "description": "Prototype implementation of PSR7 middleware for CakePHP.",
    "type": "cakephp-plugin",
    "license": "MIT",
    "require": {
        "php": ">=5.5.9",
        "zendframework/zend-diactoros": "~1.0",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*",
        "cakephp/cakephp-codesniffer": "*"
    },
    "autoload": {
        "psr-4": {
            "Spekkoek\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spekkoek\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests",
            "TestPlugin\\": "tests/TestApp/Plugin/TestPlugin/src",
            "Company\\PluginTwo\\": "tests/TestApp/Plugin/Company/PluginTwo/src"
        }
    }
}
