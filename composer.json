{
    "name": "markstory/cakephp-spekkoek",
    "description": "Prototype implementation of PSR7 middleware for CakePHP.",
    "type": "cakephp-plugin",
    "require": {
        "php": ">=5.5.9",
        "zendframework/zend-diactoros": "*",
        "cakephp/cakephp": "~3.0"
    },
    "require-dev": {
        "phpunit/phpunit": "*"
    },
    "autoload": {
        "psr-4": {
            "Spekkoek\\": "src"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spekkoek\\Test\\": "tests",
            "Cake\\Test\\": "./vendor/cakephp/cakephp/tests"
        }
    }
}
